#include <iostream>
using namespace std;
int main()
{
    int grade = 0;
    cout << "What did you get?" ;
    cin>>grade;
    if(grade>=80) {
        cout<< " A" ;
    }
   else if(75<=grade && 80>grade){
        cout<<" B+.";
    }

   else if(70<=grade && 75>grade){
    cout<<" B." ;
   }

    else if(grade>=65 && 70>grade){
        cout<< " C+" ;
    }

   else if(60<=grade && 65>grade){
        cout<<" C." ;
    }

   else if(55<=grade && 60>grade){
    cout<<" D+.";
   }

   else if(grade>=50 && 55>grade){
        cout<< " D" ;
    }
   else if(45<=grade& 50>grade){
        cout<<" E." ;
    }
    else {
        cout<<" F." ;
    }
    
    
    return 0;
}
